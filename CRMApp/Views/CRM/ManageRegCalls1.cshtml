    [data-toggle] {
        position: relative;
    }

        [data-toggle]:before {
            display: none;
            border-left: 0.3125em solid transparent;
            border-right: 0.3125em solid transparent;
            border-bottom: 0.3125em solid #1a1a1a;
            position: absolute;
            top: -0.6875em;
            left: 8.125em;
            z-index: 8;
            font-size: 0;
            line-height: 0;
            width: 0;
            height: 0;
        }

        [data-toggle]:after {
            display: none;
            content: attr(data-toggle);
            position: absolute;
            top: 1.875em;
            left: 0px;
            padding: 0 0.3125em;
            background: #1a1a1a;
            color: #fff;
            z-index: 9;
            padding-top: 1px;
            font-size: 12px;
            height: 15px;
            max-width: none;
            text-align: center;
            line-height: 1.125em;
            -webkit-border-radius: 0.1875em;
            -moz-border-radius: 0.1875em;
            border-radius: 0.1875em;
            white-space: nowrap;
            word-wrap: normal;
        }

        [data-toggle]:hover:before,
        [data-toggle]:hover:after {
            display: block;
        }

        [data-toggle].data-toggle-inner {
            max-width: none;
        }

    .arrow-position-reset {
