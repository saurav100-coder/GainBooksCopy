    .CallAccordion{text-align:right; padding-right:25px; padding-bottom:10px; margin-top:5px; cursor:pointer}
    .main_block {
        background-color: white;
        height: auto;
        padding-top: 0px;
        padding-left: 0px;
        padding-right: 0px;
        box-shadow: rgb(170, 170, 170) 5px 5px 5px;
        border-width: 2px;
        border-style: solid;
        border-color: rgb(245, 242, 242);
        border-image: initial;
        border-radius: 5px;
    }

    [data-toggle] {
        position: relative;
    }

        [data-toggle]:before {
            display: none;
            border-left: 0.3125em solid transparent;
            border-right: 0.3125em solid transparent;
            border-bottom: 0.3125em solid #1a1a1a;
            position: absolute;
            top: -0.6875em;
            left: 8.125em;
            z-index: 8;
            font-size: 0;
            line-height: 0;
            width: 0;
            height: 0;
        }

        [data-toggle]:after {
            display: none;
            content: attr(data-toggle);
            position: absolute;
            top: 1.875em;
            left: 0px;
            padding: 0 0.3125em;
            background: #1a1a1a;
            color: #fff;
            z-index: 9;
            padding-top: 1px;
            font-size: 12px;
            height: 15px;
            max-width: none;
            text-align: center;
            line-height: 1.125em;
            -webkit-border-radius: 0.1875em;
            -moz-border-radius: 0.1875em;
            border-radius: 0.1875em;
            white-space: nowrap;
            word-wrap: normal;
        }

        [data-toggle]:hover:before,
        [data-toggle]:hover:after {
            display: block;
        }

        [data-toggle].data-toggle-inner {
            max-width: none;
        }

    .arrow-position-reset {
        top: auto;
        bottom: auto;
        left: auto;
        right: auto;
        margin: 0;
    }

    body {
        font-family: Arial, Helvetica, sans-serif;
    }
    /*/* The Modal (background) */
    .modal {
        display: none; /* Hidden by default */
        position: fixed; /* Stay in place */
        /*z-index: 1;*/ /* Sit on top */
        padding-top: 100px; /* Location of the box */
        left: 0;
        top: 0;
        width: 100%; /* Full width */
        height: 100%; /* Full height */
        overflow: auto; /* Enable scroll if needed */
        background-color: rgb(0,0,0); /* Fallback color */
        background-color: rgba(0,0,0,0.4); /* Black w/ opacity */
    }
    /* Modal Content */
    .modal-content {
        background-color: #fefefe;
        margin: auto;
        padding: 20px;
        border: 1px solid #888;
        width: 40%;
        height: 170px;
        padding-top: 10px;
        bottom: 40px;
    }

    /* The Close Button */
    .close {
        color: #aaaaaa;
        float: right;
        font-size: 28px;
        font-weight: bold;
    }

        .close:hover,
        .close:focus {
            color: #000;
            text-decoration: none;
            cursor: pointer;
        }

    /*p {
        margin-top: 40px;
        text-align: center;
        font-size: 20px;
    }*/

    body {
        font-family: Source Sans Pro,Helvetica Neue,Helvetica,Arial,sans-serif;
        font-weight: 400;
        font-size: 14px;
    }

    .loader {
        margin: auto;
        border: 1em solid #f3f3f3; /* Light grey */
        border-top: 1em solid #3498db; /* Blue */
        border-radius: 50%;
        width: 7em;
        height: 7em;
        animation: spin 2s linear infinite;
        -webkit-animation: spin 2s linear infinite;
        animation: spin 2s linear infinite;
    }

    #CallEditFormDiv .form-control {
        padding: 5px;
    }
</style>

<script>
    $(document).ready(function () {
        $("nav").find(".newTitle").remove();
        var s = "<p class='newTitle' >Call Dashboard</p>";
        $("nav").find(".titleName").append(s);
        $(".NoRecord").hide();

        var CallId = '@TempData["CallId"]';
        $("#newCallId").val('@TempData["CallId"]');
        if (CallId != '' && CallId != 'undefined' && CallId != 'null' && CallId!='0') {
            $("#CallDetails").hide();
            $("#SearchPanel").css("display","none");
            document.getElementById("loadspin").style.display = "block";
            document.getElementById("overlay").style.display = "block";

            GetCallData(CallId)
            GetRemarkData(CallId)
            GetAssignData(CallId);
            GetPrevCallData(CallId)
            GetCallEngagedData(CallId)
            GetCallLogsData(CallId)
            $("#callId").val(CallId);
	         FillCallEditFormDropdowns()
        } else {
            $("#SearchPanel").css("display", "block");
            $("#CallDetails").hide();
        }

        });

</script>

