        $("#Msg").text("No record found");
        $('#loading').addClass('clickable');
    } else {
        $('#loading').hide();
        $('#loadingmessage').hide();
        $('#Msg').hide();
    }
    Deviceheight();

    $("#selectall").click(function () {
        if (this.checked) {
            $('.checkboxall').each(function () {
